    "enzyme-to-json": "^1.3.0",
    "eslint-plugin-flowtype": "2.25.0",
    "eslint-plugin-react": "6.7.1",
    "flow-bin": "0.36.0",
    "flow-typed": "2.0.0",
    "jest": "17.0.3",
    "js-platform-tools": "git+ssh://git@github.com/BrandwatchLtd/js-platform-tools.git#v1.0.7",
    "react-test-renderer": "15.4.1",
